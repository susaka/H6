$npsClients = Get-NpsRadiusClient
$changed = $false

{% for host in groups.values()%}
{% for values in host.values()%}

if($npsClients -ne {{ values }})
{
    New-NpsRadiusClient -address "{{ values.ansible_host }}" -name "{{ values }}" -sharedsecret "BmEYRA3Y"
    $changed = $true
}

{% endfor %}
{% endfor %}